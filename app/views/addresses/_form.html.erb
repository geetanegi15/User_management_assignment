<%= form_with model: [ @profile, @profile.addresses.build ] do |form| %>
    <div>
    <%= form.label :Parmanent_Address %>
    <%= form.text_field :parmanent_address %>
    </div>

    <div>
    <%= form.label :Residencial_Address %>
    <%= form.text_field :residencial_address %>
    </div>

    <div>
    <%= form.label :City %>
    <%= form.text_field :city %>
    </div>

    <div>
    <%= form.label :State %>
    <%= form.text_field :state %>
    </div>

    <div>
    <%= form.label :Pin %>
    <%= form.text_field :pin%>
    </div>    

    <div> 
    <%= form.submit " create address" %>
    </div>   
<% end %>